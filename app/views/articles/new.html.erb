<!DOCTYPE html>
<html>
  <head>
    <h1>POST A NEW ARTICLE</h1>
  </head>
  <body>
    <div>
      <%= form_for @article do |f|%>
        <%= f.text_field :title, placeholder: "Title" %>
        <%= f.text_field :info, placeholder: "Information" %>
        <%= f.submit %>
      <%end%>
    </div>
    <div>
    <%= button_to "Back", articles_path, method: :get %>
    </div>
  </body>
</html>